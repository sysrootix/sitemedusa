{
  "name": "medusa-vape-shop-backend",
  "version": "1.0.0",
  "description": "Backend API for Medusa Vape Shop with Telegram authentication",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register\" src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "db:migrate": "ts-node -r tsconfig-paths/register src/scripts/migrate.ts",
    "db:migrate:cart": "ts-node -r tsconfig-paths/register src/scripts/migrate-cart.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"Linting not configured yet\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vape",
    "shop",
    "telegram",
    "auth",
    "api",
    "nodejs",
    "typescript"
  ],
  "author": "Medusa Vape Shop",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/node-cron": "^3.0.11",
    "@types/node-forge": "^1.3.14",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.1",
    "node-telegram-bot-api": "^0.66.0",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.4.0",
    "@types/node-telegram-bot-api": "^0.64.11",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
